{
    "name": "prettier-plugin-break-empty-tags",
    "version": "0.0.5",
    "description": "A prettier plugin for support breaking the closing tag to a new line if there's no content",
    "main": "dist/index.min.js",
    "files": [
        "dist/index.min.js"
    ],
    "scripts": {
        "build": "tsc && terser dist/index.js -o dist/index.min.js -c -m",
        "release:plugin": "npm run build && npm publish"
    },
    "type": "commonjs",
    "keywords": [
        "wingedsmith",
        "prettier",
        "prettier plugin",
        "break empty tags",
        "@wingedsmith",
        "@wingedsmith/prettier-plugin-break-empty-tags",
        "prettier-plugin-break-empty-tags"
    ],
    "author": {
        "name": "wingedsmith",
        "email": "dev.vkuznetsov@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wingedsmith/prettier-plugin-break-empty-tags"
    },
    "bugs": {
        "url": "https://github.com/wingedsmith/prettier-plugin-break-empty-tags/issues"
    },
    "homepage": "https://github.com/wingedsmith/prettier-plugin-break-empty-tags#readme",
    "license": "MIT",
    "peerDependencies": {
        "prettier": "^3.7.4"
    },
    "devDependencies": {
        "@typescript-eslint/parser": "^7.6.0",
        "terser": "^5.30.3",
        "typescript": "5.3.3"
    }
}
